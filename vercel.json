{
  "version": 2,
  "name": "kosh-bangladesh-mvp",
  "buildCommand": "npm run build:vercel",
  "outputDirectory": "build",
  "routes": [
    {
      "src": "/static/(.*)",
      "dest": "/static/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ],
  "env": {
    "NODE_OPTIONS": "--openssl-legacy-provider"
  }
}
